//Multiply two decimal numbers and print the product with a precision
    of 2 decimal places .in c programming
#include <stdio.h>
int main() {
float num1, num2, product;
printf("Enter two decimal numbers: ");
scanf("%f %f", &num1, &num2);
product = num1 * num2;
printf("Product: %.2f\n", product);
return 0;
}

